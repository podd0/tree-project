.\pointgen.exe --file ..\examples\bunny\bunny.ply --shape generic_shape --samples 250000 --output ..\examples\bunny\points.ply

.\tree.exe --input ..\examples\bunny\points.ply --enable_leaves --leaf ..\examples\leaves\leaf.ply --output  ..\examples\bunny\tree.ply --leaves_output  ..\examples\bunny\leaves.ply --br_length 0.001 --kill 0.007 --attraction 0.1 --inverted_growth 3 --leaf_radius 0.00018 --leaf_scale 0.00003